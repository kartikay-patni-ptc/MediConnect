{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/*.html",
					"/*.css",
					"/*.js"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**",
					"/manifest.webmanifest"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "api",
			"urls": [
				"http://localhost:8080/api/**"
			],
			"cacheConfig": {
				"strategy": "performance",
				"maxSize": 100,
				"maxAge": "1d",
				"timeout": "10s"
			}
		}
	]
}



